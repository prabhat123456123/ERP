<% if (error.length>0) { %>
<div class="alert alert-danger " style="width: 70%; margin-left: auto; margin-right: auto;" id="error">
  <span><%= error %></span>
</div>
<% } %> <% if (error_msg.length>0) { %>
<div class="alert alert-danger " style="width: 70%; margin-left: auto; margin-right: auto;" id="error_msg">
  <span id="errorMessage"><%= error_msg %></span>
</div>
<% } %> <% if (success_msg.length>0) { %>
<div class="alert alert-success" style="width: 70%; margin-left: auto; margin-right: auto;" id="success_msg">
  <span id="message"><%= success_msg %></span>
</div>
<% } %>
